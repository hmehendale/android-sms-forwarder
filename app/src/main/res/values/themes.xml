<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Messy" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/olivine</item>
        <item name="colorPrimaryVariant">@color/midnightgreen</item>
        <item name="colorOnPrimary">@color/charcoal</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/tangerine</item>
        <item name="colorSecondaryVariant">@color/flame</item>
        <item name="colorOnSecondary">@color/charcoal</item>

        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
    </style>
</resources>